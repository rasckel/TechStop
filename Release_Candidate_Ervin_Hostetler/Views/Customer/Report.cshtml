@model IEnumerable<Release_Candidate_Ervin_Hostetler.Models.Quote>

@{
    ViewData["Title"] = "Report";
}

<h2>Report</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                <a asp-action="Edit" asp-route-id="@item.QuoteID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.QuoteID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.QuoteID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
